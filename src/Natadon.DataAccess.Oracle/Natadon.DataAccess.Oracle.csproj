<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Company>Natadon Technologies</Company>
    <Authors>Kyle Parrish</Authors>
    <Copyright>2021</Copyright>
    <Description>An implementation of the Natadon.DataAccess for Oracle databases.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/Natadon/DataAccess.Oracle</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Natadon/DataAccess.Oracle</RepositoryUrl>
    <PackageTags>DataAccess</PackageTags>

    <Version>1.0.4</Version>

    <AssemblyVersion>1.0.2.0</AssemblyVersion>

    <FileVersion>1.0.2.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Natadon.DataAccess.Core" Version="1.0.1" />
    <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="3.21.61" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
